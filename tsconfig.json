{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@eqeq/app/*": ["./src/*"],
      "@eqeq/models/*": ["./src/models/*"],
      "@eqeq/components/*": ["./src/components/*"],
      "@eqeq/containers/*": ["./src/containers/*"],
      "@eqeq/views/*": ["./src/views/*"]
    },
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "module": "es2015",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "build/dist",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": ["build", "node_modules"],
  "types": ["typePatches"]
}
